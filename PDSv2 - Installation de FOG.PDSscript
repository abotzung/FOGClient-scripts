# PDSv2 - Installe le client FOG + FOGPatcherV3 (Janvier 2025)
#
# Un SED dégueulasse, mais qui fait le boulot !
FOGSrvIP=$(echo "${web}" | awk -F/ '{print $3}' | sed 's/:.*//')
FOGSrvADDR=$(echo "${web}" | awk -F"$FOGSrvIP" '{print $2}')

#Télécharge le client && mets-le dans le dossier qui va bien
curl -Lks -o /ntfs/temp/fogclient.exe ${web}/client/download.php?smartinstaller

# Un peu de nettoyage...
if [ -d "/ntfs/Program Files (x86)/FOG" ]; then
	rm -rf "/ntfs/Program Files (x86)/FOG"
fi
if [ -f "/ntfs/fog.log" ]; then
	rm -rf "/ntfs/fog.log"
fi

destFile="/ntfs/temp/tasks.ps1"

cat <<'EOF' >> "$destFile"
# Tache : Installe le client FOG et le patch automatiquement
function InstalleEtPatchFOGSvc() {
	$NetPresent=0
	$Counter=0

	PDS_WriteText -msg 'Configuration de FOG, merci de patienter...'

	# Attente de 60 secondes, la présence du réseau
	# $NetPresent == 1 si il y a du réseau.
	for (($Counter = 1); $Counter -lt 61; $Counter++)
	{
EOF
echo "	    ping ${FOGSrvIP} -w 1000 -n 1 > \$Null" >> "$destFile"
cat <<'EOF' >> "$destFile"
		if ($LASTEXITCODE -eq '0') { $NetPresent=1 ; $Counter=9999;} else {PDS_WriteText -msg "Attente du réseau...$Counter/60 seconde(s)" }
	}
	PDS_WriteText -msg 'Réseau présent, Installation de FOG...'
EOF
if [[ "${web}" ==  *"https"* ]]; then
	echo "	Start-Process -FilePath \"c:\\temp\\fogclient.exe\" -WorkingDirectory \"c:\\temp\\\" -Wait -WindowStyle Hidden -ArgumentList \"--server=${FOGSrvIP} --webroot=${FOGSrvADDR} -h -t\"" >> "$destFile"
else
	echo "	Start-Process -FilePath \"c:\\temp\\fogclient.exe\" -WorkingDirectory \"c:\\temp\\\" -Wait -WindowStyle Hidden -ArgumentList \"--server=${FOGSrvIP} --webroot=${FOGSrvADDR} -t\"" >> "$destFile"
fi

cat <<'EOF' >> "$destFile"
	sleep 1
	try
	{
		$FOGSvcExec = (Get-ItemPropertyValue -Path Registry::HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\FOGService -Name ImagePath)
		$FOGSvcExec = $FOGSvcExec -replace '"',''
		$FOGSvcPath = Split-Path -Path $FOGSvcExec
		$FOGSvcPath = $FOGSvcPath+'\'
		
		# Coupe FOG
		sc.exe stop FOGService
		sc.exe config "FOGService" start=disabled 
		# Arrive que le service redémarre pendant sa maintenance, on essaye de le couper
		sc.exe stop FOGService
		taskkill.exe /F /IM FOGService.exe 
		taskkill.exe /F /IM FOGTray.exe 
		taskkill.exe /F /IM FOGUserService.exe
		taskkill.exe /F /IM FOGUpdateWaiter.exe 
		taskkill.exe /F /IM FOGUpdateHelper.exe 
		taskkill.exe /F /IM FOGShutdownGUI.exe
		sc.exe stop FOGService
		Start-Process -FilePath "powershell.exe" -WorkingDirectory "c:\temp\" -Wait -WindowStyle Hidden -ArgumentList "-ExecutionPolicy bypass -File c:\temp\FOGPatcher.ps1"
		sc.exe config "FOGService" start=delayed-auto

		PDS_WriteText -msg "Configuration de FOG terminée"
	}
	catch
	{
		PDS_WriteText -msg "ERREUR : L'installation de FOG à échouée."
		# ...car on ne retrouve PAS la clé du service FOG dans le registre (=PAS de service installé ; donc PAS de FOG)
		# ... généralement les installations échouent car : 
		#  - "Unable to install CA certificate"
		#  - ..qui peut être un problème réseau. (CLIENT ou SERVEUR)
		#  - Un problème de droits ? J'en doute, mais reste à tester :)
	}

	 
	Remove-Item -Path "c:\temp\fogclient.exe" -Force
	Remove-Item -Path "c:\temp\FOGPatcher.ps1" -Force
}
$PDSTASKS+="InstalleEtPatchFOGSvc;"
EOF

# -=-=-=-=-=--=-=-=-=-=- Copie FOGPatcher dans le répertoire temporaire
cat <<'EOF' >> "/ntfs/temp/FOGPatcher.ps1"
# FOGPatcher by Alexandre BOTZUNG (alexandre.botzung@GrandEst.fr) - Fév2023..Jan2025
# 
# Provides a way to execute powershell script before reboot/shutdown initiated by FOG Client (Windows only)
#
# Credit:
# Source : https://stackoverflow.com/questions/73790902/replace-string-in-a-binary-clipboard-dump-from-onenote
# Author : mklement0
# 
# Source : https://stackoverflow.com/questions/18917754/in-powershell-can-i-use-the-select-string-quiet-switch-within-a-foreach-stateme
# Author : CB.

function StopFOG() {
	# Kill all FOG Processes 
	Stop-Service -Name FOGService -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGShutdownGUI" -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGTray" -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGUserService" -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGUpdateHelper" -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGUpdateWaiter" -Force -ErrorAction SilentlyContinue
	Stop-Process -Name "FOGService" -Force -ErrorAction SilentlyContinue	
}

Write-host 'Pre reboot/shutdown FOG Client patcher - Alexandre BOTZUNG (alexandre.botzung@grandest.fr) ver. : 20250124'

$PSScript = $MyInvocation.MyCommand
if(!$PSScriptRoot){$PSScriptRoot = Split-Path $MyInvocation.MyCommand.Path -Parent}

$PatchingDone = '0'

$SYREBOOT_DLL_MD5 = '8A7036A411131C6DCAC3EEE26F386F14'
$SYREBOOT_DLL = @'

'@

$PREREBOOT_PS1_MD5 = 'EA5CD5F6471C3240B760D0B721F9C49C'
$PREREBOOT_PS1 = @'

'@

try
{
	$FOGSvcExec = (Get-ItemPropertyValue -Path Registry::HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\FOGService -Name ImagePath)
	$FOGSvcExec = $FOGSvcExec -replace '"',''
	$FOGSvcPath = Split-Path -Path $FOGSvcExec
	$FOGSvcPath = $FOGSvcPath+'\'

	$Zazzles = $FOGSvcPath+'Zazzles.dll'
}
catch
{
	Throw "FOG Client service not found on this host."
	exit 1
}

$search = 'advapi32'
$byteEncParam = 
  if ($IsCoreCLR) { @{ AsByteStream = $true } } 
  else            { @{ Encoding = 'Byte' } }
# Patch 'advapi32' -> 'syreboot' (syreboot.dll)
$data = Get-Content @byteEncParam -ReadCount 0  $Zazzles
$dataAsHexString = [BitConverter]::ToString($data)
$searchAsHexString = [BitConverter]::ToString([Text.Encoding]::UTF8.GetBytes($search))
$replaceAsHexString = [BitConverter]::ToString([Text.Encoding]::UTF8.GetBytes('syreboot'))
If ($dataAsHexString -match $searchAsHexString ) 
{
	StopFOG
	Copy-Item $Zazzles -Destination $Zazzles'.bak'
	Write-host $search' found, patching.'
	$dataAsHexString = $dataAsHexString.Replace($searchAsHexString, $replaceAsHexString)
	$modifiedData = [byte[]] ($dataAsHexString -split '-' -replace '^', '0x')
	try {
		Set-Content @byteEncParam $Zazzles -Value $modifiedData
	}
	catch {
		Write-host 'ERROR ! : Unable to patch '$Zazzles' with keyword '$search
		exit 1
	}
	$PatchingDone = '1'
}
# Patch 'InitiateSystemShutdownEx' -> 'InitiateUEFIBeforeReboot'
$search = 'InitiateSystemShutdownEx'
$data = Get-Content @byteEncParam -ReadCount 0  $Zazzles
$dataAsHexString = [BitConverter]::ToString($data)
$searchAsHexString = [BitConverter]::ToString([Text.Encoding]::UTF8.GetBytes($search))
$replaceAsHexString = [BitConverter]::ToString([Text.Encoding]::UTF8.GetBytes('InitiateUEFIBeforeReboot'))
If ($dataAsHexString -match $searchAsHexString ) 
{
	StopFOG
	Write-host $search' found, patching.'
	$dataAsHexString = $dataAsHexString.Replace($searchAsHexString, $replaceAsHexString)
	$modifiedData = [byte[]] ($dataAsHexString -split '-' -replace '^', '0x')
	try {
		Set-Content @byteEncParam $Zazzles -Value $modifiedData
	}
	catch {
		Write-host 'ERROR ! : Unable to patch '$Zazzles' with keyword '$search
		exit 1
	}
	$PatchingDone = '1'
}
if (Test-Path $FOGSvcPath'syreboot.dll') {
	if (((Get-FileHash -Algorithm MD5 $FOGSvcPath'syreboot.dll').hash) -ne $SYREBOOT_DLL_MD5) {
		Write-host 'File syreboot.dll needs to be updated'
		$PatchingDone = '1'
	}
} else {
	$PatchingDone = '1'
}
if (Test-Path $FOGSvcPath'prereboot.ps1') {
	if (((Get-FileHash -Algorithm MD5 $FOGSvcPath'prereboot.ps1').hash) -ne $PREREBOOT_PS1_MD5) {
		Write-host 'File prereboot.ps1 needs to be updated'
		$PatchingDone = '1'
	}
} else {
	$PatchingDone = '1'
}
if ($PatchingDone -eq '1') 
{
	StopFOG
	Write-host "Patching detected, i'm gonna :"
	Write-host ' -> Create file '$FOGSvcPath'syreboot.dll'
	$bytes = [Convert]::FromBase64String($SYREBOOT_DLL)
	[IO.File]::WriteAllBytes($FOGSvcPath+'syreboot.dll', $bytes)

	Write-host ' -> Create file '$FOGSvcPath'prereboot.ps1'
	$bytes = [Convert]::FromBase64String($PREREBOOT_PS1)
	[IO.File]::WriteAllBytes($FOGSvcPath+'prereboot.ps1', $bytes)
	
	Write-host ' -> Restart service fogservice'
	Restart-Service -Name 'FOGService' -Force -ErrorAction SilentlyContinue
	
	Write-Host ' -> Copy this script into '$FOGSvcPath

	Copy-Item $PSScriptRoot\$PSScript -Destination $FOGSvcPath'FOGPatcher.ps1' -Force -ErrorAction SilentlyContinue
	
	Write-host ' -> Disable and remove scheduled tasking "FOGPatcher"'

	Stop-ScheduledTask -TaskName "FOGPatcher" -ErrorAction SilentlyContinue
	Disable-ScheduledTask -TaskName "FOGPatcher" -ErrorAction SilentlyContinue
	Unregister-ScheduledTask -TaskName "FOGPatcher" -Confirm:$false -ErrorAction SilentlyContinue

	Write-host 'Done, goodbye.'
} else {
	Write-host 'Nothing to do. Goodbye'
}
exit 0

# Source-code of syreboot.dll (Purebasic file / syreboot.pb)
#$SYREBOOT_PB = @'
#
#'@
#$bytes = [Convert]::FromBase64String($SYREBOOT_PB)
#[IO.File]::WriteAllBytes('syreboot.pb', $bytes)
# ---------------------------
EOF
#Fin